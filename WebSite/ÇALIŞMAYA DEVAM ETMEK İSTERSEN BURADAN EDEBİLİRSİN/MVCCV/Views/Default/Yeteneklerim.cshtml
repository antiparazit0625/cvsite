@using MVCCV.Models.Entity;
@model List<TblYeteneklerim>
<div class="resume-section-content">
    <h2 class="mb-5">Yeteneklerim</h2>
    <div class="subheading mb-3">Programlama Dilleri ve Araçlar</div>
    <ul class="list-inline dev-icons">
        <li class="far fa-file-excel"></li>
        <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
        <li class="fas fa-chart-area"></li>
        <li class="fas fa-chart-pie"></li>
        <li class="fas fa-cog"></li>
        <li class="list-inline-item"><i class="fab fa-react"></i></li>
        <li class="fas fa-code"></li>
        <li class="list-inline-item"><i class="fab fa-node-js"></i></li>
        <li class="fas fa-memory"></li>
        <li class="fab fa-windows"></li>
        <li class="list-inline-item"><i class="fab fa-js-square"></i></li>
        <li class="list-inline-item"><i class="fab fa-angular"></i></li>
       
    </ul>
    <div class="subheading mb-3">İş Akışı</div>
    <ul class="fa-ul mb-0">
        @foreach (var x in Model)
        {
            <li>
                <span class="fa-li"><i class="fas fa-check"></i></span>
               @x.Yetenek
            </li>
        }
    </ul>
</div>