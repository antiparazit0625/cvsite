@using MVCCV.Models.Entity
@model List<TblLogın>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Admin İşlemleri</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Kullanıcı Adı</th>
        <th>Düzenle</th>
        <th>Sil</th>
    </tr>
    @foreach (var x in Model)
    {
    <tr>
        <td>@x.ID</td>
        <td>@x.KullanıcıAdi</td>
        <td><a href="/Admin/Duzenle/@x.ID" class="btn progress-bar-striped btn-info">Düzenle</a></td>
        <td><a href="/Admin/Sil/@x.ID" class=" btn progress-bar-striped btn-warning">Sil</a></td>
    </tr>
    }
</table>
<a href="/Admin/Ekle/" class="btn btn-primary">Yeni Kullanıcı Ekle</a>